{
  "name": "react-formula",
  "author": "User Friendly",
  "homepage": "https://github.com/user-friendly",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build-dev": "vite build --mode development",
    "build-qa": "vite build --mode testing",
    "build-stage": "vite build --mode staging",
    "build": "vite build",
    "lint": "eslint .",
    "lint-force-color": "eslint . --color",
    "format": "prettier --write src/ msw/",
    "format-check": "prettier -c src/ msw/",
    "preview": "vite preview",
    "msw-test": "cd msw && rollup -c rollup.config.js --watch --watch.onEnd='node dist/bundle.js'"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@rollup/pluginutils": "^5.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "lodash": "^4.17.21",
    "mersennetwister": "^0.2.3",
    "msw": "^2.4.10",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.10",
    "uuid": "^10.0.0",
    "vite": "^5.4.1"
  },
  "imports": {
    "#*": "/src/*",
    "#cap/*": "/src/Standalone/Capstone/*"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
